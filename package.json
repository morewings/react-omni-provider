{
  "name": "react-omni-provider",
  "homepage": "https://github.com/morewings/react-omni-provider",
  "license": "MIT",
  "private": false,
  "version": "0.1.2",
  "type": "module",
  "files": [
    "dist"
  ],
  "main": "./dist/index.umd.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "sideEffects": false,
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.umd.cjs"
    }
  },
  "keywords": [
    "react",
    "provider"
  ],
  "description": "Missing React, typescript NPM library creation template",
  "scripts": {
    "dev": "vite",
    "clean:lib": "rm -rf dist",
    "build:lib": "vite build",
    "build": "run-s clean:lib build:lib",
    "lint:code": "eslint ./src/ --ext ts,tsx --report-unused-disable-directives",
    "fix:code": "run-s 'lint:code --fix'",
    "lint:types": "tsc --pretty --noEmit",
    "test": "jest",
    "coverage": "run-s 'test --coverage'",
    "preview": "vite preview",
    "prepare": "is-ci || husky",
    "lint-staged": "lint-staged",
    "commitlint": "commitlint",
    "semantic-release": "semantic-release"
  },
  "peerDependencies": {
    "react": ">=18.2.0",
    "react-dom": ">=18.2.0"
  },
  "dependencies": {
    "lodash": "^4.17.21"
  },
  "devDependencies": {
    "@commitlint/cli": "19.2.2",
    "@commitlint/config-conventional": "19.2.2",
    "@semantic-release/commit-analyzer": "^12.0.0",
    "@semantic-release/git": "^10.0.1",
    "@semantic-release/github": "^10.0.0",
    "@semantic-release/npm": "^12.0.0",
    "@semantic-release/release-notes-generator": "^13.0.0",
    "@testing-library/jest-dom": "6.4.2",
    "@testing-library/react": "15.0.2",
    "@types/jest": "29.5.12",
    "@types/lodash": "4.17.0",
    "@types/react": "18.2.79",
    "@types/react-dom": "18.2.25",
    "@typescript-eslint/eslint-plugin": "7.7.0",
    "@typescript-eslint/parser": "6.21.0",
    "@vitejs/plugin-react": "4.2.1",
    "@yelo/rollup-node-external": "^1.0.1",
    "alias-hq": "6.2.3",
    "commitizen": "4.3.0",
    "commitlint": "19.2.2",
    "cz-conventional-changelog": "3.3.0",
    "eslint": "8.57.0",
    "eslint-config-prettier": "9.1.0",
    "eslint-plugin-import": "2.29.1",
    "eslint-plugin-prettier": "5.1.3",
    "eslint-plugin-react": "7.34.1",
    "eslint-plugin-react-hooks": "4.6.0",
    "eslint-plugin-react-refresh": "0.4.6",
    "eslint-plugin-ssr-friendly": "1.3.0",
    "eslint-plugin-storybook": "0.8.0",
    "husky": "9.0.11",
    "identity-obj-proxy": "3.0.0",
    "is-ci": "3.0.1",
    "jest": "29.7.0",
    "jest-environment-jsdom": "29.7.0",
    "lint-staged": "15.2.2",
    "npm-run-all2": "5.0.0",
    "postcss": "8.4.38",
    "postcss-preset-env": "^9.5.3",
    "prettier": "3.2.5",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "semantic-release": "23.0.8",
    "ts-jest": "29.1.2",
    "ts-node": "10.9.2",
    "typescript": "5.3.3",
    "vite": "5.2.10",
    "vite-plugin-dts": "^3.8.1"
  }
}
